<aura:component >
	<aura:attribute name="Street" type="String" default="Weymouth"/>
	<aura:attribute name="City" type="String" default="London"/>
	<aura:attribute name="State" type="String" default="Middlesex"/>
	<aura:attribute name="PostalCode" type="String" default="W1 1RD"/>    
    Street {!v.Street}
    City {!v.City}
    State {!v.State}    
    PostalCode {!v.PostalCode} 
</aura:component>